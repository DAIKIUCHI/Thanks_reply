<!DOCTYPE html>
<html>
  <head>
    <title>ThanksReply</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if user_signed_in? %>
      <%= current_user.nickname %>/ 
      <%= current_user.name %>
    <% end %>
    <%= yield %>
  </body>
</html>
